<div class="container">
  <div class="text-center">
    <h2>Wikinimous Articles:</h2>
    <ul>
      <% @articles.each do |article| %>
      <li>
        <%= link_to(article.title, article_path(id: article.id))%> <%= article.content%>
      </li>
      <% end %>
    </ul>
    <%= link_to "Create a new article", new_article_path%>
  </div>
</div>
